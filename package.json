{
  "name": "v-template",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun vite dev --port 3000",
    "build": "bunx --bun vite build",
    "start": "bun run .output/server/index.mjs",
    "db:push": "bunx --bun drizzle-kit push",
    "db:studio": "bunx --bun drizzle-kit studio",
    "auth:generate": "bunx --bun @better-auth/cli@latest generate --output ./server/schema/auth.ts --config ./server/auth.ts -y && sed -i '' -E 's/\\(\"([^\"]*)\"\\)/()/g' ./server/schema/auth.ts"
  },
